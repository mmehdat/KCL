<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
<SelectDialog
        id = "MatTypeVH"
        title="Material Type"
        class="sapUiPopupWithPadding"
        items="{VHModelResult>/}"
        search="_handleMatTypeValueHelpSearch"
        confirm="_handleMatTypeValueHelpClose"
        cancel="_handleMatTypeValueHelpClose"       
        rememberSelections="false"
        showClearButton="true"
        multiSelect="true">
        <StandardListItem
            icon="{ProductPicUrl}"
            iconDensityAware="false"
            iconInset="false"
            title="{VHModelResult>MaterialType}"
            description="{VHModelResult>MaterialTypeDesc}" />
    </SelectDialog>
</core:FragmentDefinition>